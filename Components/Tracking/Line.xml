<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<Frame name="PetTrackerLine" inherits="ObjectiveTrackerLineTemplate" virtual="true">
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="Icon">
					<Size x="16" y="16"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="-1"/>
					</Anchors>
				</Texture>
			</Layer>

			<Layer level="OVERLAY">
				<Texture parentKey="SubIcon">
					<Size x="12" y="12"/>
					<Anchors>
						<Anchor point="CENTER" relativeKey="$parent.Icon" relativePoint="BOTTOMRIGHT" x="-1" y="3"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
</Ui>